{
  "manifest_version": 2,

  "name": "Vaultpass",
  "description": "A password generator based on domain and your passphrase.",
  "version": "0.4.1",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "19":  "icon.png",
      "38":  "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons":
    {
      "19":  "icon.png",
      "38":  "icon.png",
      "128": "icon.png"
    }
}
